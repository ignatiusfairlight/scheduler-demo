{
    "extends": "./.svelte-kit/tsconfig.json",
    "compilerOptions": {
      "types": ["svelte"],
      "target": "ESNext",
      "module": "ESNext",
      "moduleResolution": "bundler",
      "esModuleInterop": true,
      "strict": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src"]
  }
  
  